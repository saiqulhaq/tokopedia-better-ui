{
  "name": "Hello, World!",
  "version": "1.0",
  "manifest_version": 3,
  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },
  "content_scripts": [
    {
      "js": [
        "main.js"
      ],
      "matches": [
        "https://*.tokopedia.com/*",
        "https://tokopedia.com/*",
        "*://tokopedia.com/*"
      ]
    }
  ],
  "permissions": ["declarativeNetRequest", "*://gql.tokopedia.com/*"],
  "declarative_net_request" : {
    "rule_resources" : [{
      "id": "ruleset_1",
      "enabled": true,
      "path": "rules_1.json"
    }]
  }
}
